{
  "name": "wiregui",
  "productName": "wiregui",
  "version": "1.8.6",
  "description": "A graphical user interface for wireguard",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": "devsfy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/henri26mobu/wiregui"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icons/icon.png",
        "executableName": "wiregui"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "wiregui",
            "icon": "./src/assets/icons/icon.ico",
            "setupIcon": "./src/assets/icons/icon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "icon": "./src/assets/icons/icon.png",
              "scripts": {
                "postinst": "./scripts/postinst.sh",
                "postrm": "./scripts/postrm.sh"
              },
              "extraFiles": [
                {
                  "src": "./policies/org.wiregui.toggle.policy",
                  "dest": "/usr/share/wiregui/org.wiregui.toggle.policy"
                }
              ]
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "options": {
              "icon": "./src/assets/icons/icon.png"
            }
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./src/assets/icons/icon.png",
            "format": "ULFO"
          }
        },
        {
          "name": "electron-forge-maker-appimage",
          "platforms": [
            "linux"
          ],
          "config": {
            "options": {
              "icon": "./src/assets/icons/icon.png"
            }
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Devsfy",
              "name": "wiregui"
            }
          }
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-dmg": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-webpack": "^7.11.1",
    "@electron-forge/publisher-github": "^7.11.1",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^4.2.1",
    "electron": "^28.3.3",
    "electron-forge-maker-appimage": "https://github.com/Marcus10110/electron-forge-maker-appimage.git",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "node-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@types/qrcode": "^1.5.6",
    "date-fns": "^2.21.1",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^10.18.0",
    "immer": "^11.1.4",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^11.0.5",
    "recoil": "^0.7.7",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sudo-prompt": "^9.2.1",
    "update-electron-app": "^3.1.2",
    "wireguard-tools": "^0.1.0"
  }
}
